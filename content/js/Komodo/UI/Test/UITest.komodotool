    "/** Pane ui", 
    " *", 
    " * @return self", 
    " */", 
    "var paneUI = function()", 
    "{", 
    "\tvar document_pane\t= ko.extensions.TemplateExtension.Komodo.Document.get('pane');", 
    "\t", 
    "\treturn new ko.extensions.TemplateExtension.Komodo.UI().document(document_pane);", 
    "}; ", 
    "", 
    "/** Remove controls", 
    " *", 
    " */", 
    "var removeControls = function()", 
    "{", 
    "\tpaneUI().$( '#te_controls_box' ).empty();", 
    "}; ", 
    "", 
    "/** Add controls", 
    " */", 
    "var addControls = function()", 
    "{", 
    "\tvar UI = paneUI();", 
    "\t", 
    "\tUI.append('#te_controls_box',", 
    "\t\tUI.create('checkbox', [", 
    "\t\t\t{label: 'Checkbox 1', prefs:false},", 
    "\t\t\t{label: 'Checkbox 2', prefs:false},", 
    "\t\t])", 
    "\t)", 
    "\t.append('#te_controls_box',", 
    "\t\tUI.create('checkbox', [", 
    "\t\t\t{label: 'Checkbox prefs 1', checked:true},", 
    "\t\t\t{label: 'Checkbox prefs 2', prefs:true},", 
    "\t\t])", 
    "\t);", 
    "\t", 
    "};", 
    "/** Get values", 
    " */", 
    "var getAllValues = function()", 
    "{", 
    "\tconsole.log( paneUI().values( '#te_controls_box') );", 
    "}; ", 
    "/** Get prefs values", 
    " */", 
    "var getPrefsValues = function()", 
    "{", 
    "\tconsole.log( paneUI().values( '#te_controls_box', 'prefs' ) );", 
    "}; ", 
    "/*---------------------------------------", 
    "\tRUN TESTS", 
    "-----------------------------------------", 
    "*/", 
    "removeControls();", 
    "addControls();", 
    "getAllValues();", 
    "getPrefsValues();"
  ], 
