    "", 
    "/** Pane ui", 
    " *", 
    " * @return self", 
    " */", 
    "var paneUI = function()", 
    "{", 
    "\tvar document_pane\t= ko.extensions.TemplateExtension.Komodo.Document.get('pane');", 
    "\t", 
    "\treturn new ko.extensions.TemplateExtension.Komodo.UI().document(document_pane);", 
    "}; ", 
    "", 
    "/** Remove controls", 
    " *", 
    " */", 
    "var removeControls = function()", 
    "{", 
    "\tpaneUI().$( '#te_controls_box' ).empty();", 
    "}; ", 
    "", 
    "/** Add controls", 
    " */", 
    "var addControls = function()", 
    "{", 
    "\tvar UI = paneUI();", 
    "\t", 
    "\tUI.append('#te_controls_box',", 
    "\t\tUI.create('checkbox', [", 
    "\t\t\t{label: 'Checkbox 1', prefs:false},", 
    "\t\t\t{label: 'Checkbox 2', prefs:false},", 
    "\t\t])", 
    "\t)", 
    "\t.append('#te_controls_box',", 
    "\t\tUI.create('checkbox', [", 
    "\t\t\t{label: 'Checkbox prefs 1', checked:true},", 
    "\t\t\t{label: 'Checkbox prefs 2', prefs:true},", 
    "\t\t])", 
    "\t);", 
    "\t", 
    "};", 
    "/** Create pref set", 
    " */", 
    "var createPrefSet = function()", 
    "{", 
    "\t/** Create prefset wrapper element", 
    "\t */", 
    "\tvar createPrefsetParentElement = (function()", 
    "\t{", 
    "\t\tvar UI = paneUI();", 
    "\t\tUI.append( '#TemplateExtension-pane > hbox',\tUI.create('hbox', {id:'te_controls_box'}) );", 
    "\t\tUI.append( '#te_controls_box',\tUI.create('vbox', {id:'pref_set_test'}) );", 
    "\t})(); ", 
    "\t", 
    "\t", 
    "\tvar template\t= {groupbox: ['checkbox', 'checkbox']};", 
    "\tvar values\t= {'conteiner-1-A':{", 
    "\t\t\t\t'Control A 1': true,", 
    "\t\t\t\t'Control A 2': false,", 
    "\t\t\t},", 
    "\t\t  'conteiner-1-B':{", 
    "\t\t\t\t'Control B 1': false,", 
    "\t\t\t\t'Control B 2': true,", 
    "\t\t\t},", 
    "\t\t  };\t", 
    "", 
    "\tpaneUI().createPrefSet('#pref_set_test', template, values );", 
    "}; ", 
    "", 
    "", 
    "", 
    "/** Get values", 
    " */", 
    "var getAllValues = function()", 
    "{", 
    "\tconsole.log( paneUI().values( '#te_controls_box') );", 
    "}; ", 
    "/** Get prefs values", 
    " */", 
    "var getPrefsValues = function()", 
    "{", 
    "\tconsole.log( paneUI().values( '#te_controls_box', 'prefs' ) );", 
    "}; ", 
    "/*---------------------------------------", 
    "\tRUN TESTS", 
    "-----------------------------------------", 
    "*/", 
    "removeControls();", 
    "//addControls();", 
    "//getAllValues();", 
    "//getPrefsValues();", 
    "", 
    "createPrefSet();", 
    "", 
    "", 
    "", 
    "", 
    "", 
