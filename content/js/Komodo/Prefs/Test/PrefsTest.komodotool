    "var prefs\t= ko.extensions.TemplateExtension.Komodo.Prefs;", 
    "var values = {", 
    "\t'string':\t'default string', ", 
    "\t'integer':\t999999999, ", 
    "\t'float':\t1.1111111, ", 
    "\t'boolean':\ttrue, ", 
    "\t'null':\tnull, ", 
    "\t'empty':\t'',", 
    "\t'object':\t{'string':'string value', 'boolean':true, 'nested-array':['item-1']},", 
    "\t'array':\t['item-1',['subarray item']],", 
    "};", 
    "/** ", 
    " */", 
    "var defaultValues = function()", 
    "{", 
    "\tvar prefix\t= 'pref-test-defaults-';", 
    "", 
    "\tfor(var type in values)", 
    "\t\tif (values.hasOwnProperty(type))", 
    "\t\t\tconsole.log( 'Test defaults '+type+': '+(values[type] === prefs.get( prefix+type, values[type] )) );\t\t", 
    "}; ", 
    "", 
    "/** Get pane values", 
    " */", 
    "var testPrefs = function(method)", 
    "{", 
    "\tvar prefix\t= 'pref-test-';", 
    "\t", 
    "\tfor(var type in values)", 
    "\t\tif (values.hasOwnProperty(type)){", 
    "\t\t\tif( method!=='get' )", 
    "\t\t\t\tprefs[method]( prefix+type, values[type] );", 
    "\t\t\telse{", 
    "\t\t\t\tvar value\t= prefs.get(prefix+type);", 
    "\t\t\t\tconsole.log( 'Test '+type+': '+ (value===values[type] || typeof value === typeof values[type]) );", 
    "\t\t\t}\t\t\t\t\t\t\t", 
    "\t\t}", 
    "};", 
    "", 
    "", 
    "/*---------------------------------------", 
    "\tRUN TEST", 
    "-----------------------------------------", 
    "*/", 
    "defaultValues();", 
    "", 
    "testPrefs('set');", 
    "testPrefs('get');", 
    "testPrefs('delete');"
